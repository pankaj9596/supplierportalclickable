<mvc:View controllerName="com.spm.suppilerportal.controller.SupplierOnboarding" xmlns:upload="sap.m.upload" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m" xmlns:layout="sap.ui.layout" xmlns:form="sap.ui.layout.form" xmlns:micro="sap.suite.ui.commons"
	xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" title="{i18n>title}" enableScrolling="false" backgroundDesign="List">
		<customHeader>
			<Bar>
				<contentLeft>
					<Button type="Emphasized" icon="sap-icon://home" press="onPressHomePage"/>
				</contentLeft>
			</Bar>
		</customHeader>
		<content>
			<ScrollContainer vertical="true" height="100%">
				<HBox backgroundDesign="Transparent" justifyContent="End" width="100%">
					<Image src="/css/image/toplogo.PNG"/>
				</HBox>
				<FlexBox justifyContent="Center" width="100%">
					<micro:MicroProcessFlow id="idMP">
						<micro:content>
							<micro:MicroProcessFlowItem class="suplierCss classPurpledot1"/>
							<micro:MicroProcessFlowItem class="suplierCss classPurpledot" press="onfnnextpress1"/>
							<micro:MicroProcessFlowItem class="suplierCss" press="onfnnextpress2"/>
							<micro:MicroProcessFlowItem class="suplierCss" press="onfnnextpress3"/>
							<micro:MicroProcessFlowItem class="suplierCss" press="onfnnextpress4"/>
							<micro:MicroProcessFlowItem class="suplierCss" press="onfnnextpress5"/>
							<micro:MicroProcessFlowItem class="suplierCss" press="onfnnextpress6"/>
							<micro:MicroProcessFlowItem class="suplierCss" press="onfnnextpress7"/>
							<micro:MicroProcessFlowItem class="suplierCss" press="onfnnextpress8"/>
							<micro:MicroProcessFlowItem class="suplierCss" press="onfnnextpress9"/>
							<micro:MicroProcessFlowItem class="suplierCss" press="onfnnextpress10"/>
						</micro:content>
					</micro:MicroProcessFlow>
				</FlexBox>
				<core:Fragment fragmentName="com.spm.suppilerportal.view.subView.SupplierOnboarding.BasicInformation" type="XML"/>
				<core:Fragment fragmentName="com.spm.suppilerportal.view.subView.SupplierOnboarding.Department" type="XML"/>
				<core:Fragment fragmentName="com.spm.suppilerportal.view.subView.SupplierOnboarding.Address" type="XML"/>
				<core:Fragment fragmentName="com.spm.suppilerportal.view.subView.SupplierOnboarding.ContactList" type="XML"/>
				<core:Fragment fragmentName="com.spm.suppilerportal.view.subView.SupplierOnboarding.AccountInfo" type="XML"/>
				<core:Fragment fragmentName="com.spm.suppilerportal.view.subView.SupplierOnboarding.PaymentInfo" type="XML"/>
				<core:Fragment fragmentName="com.spm.suppilerportal.view.subView.SupplierOnboarding.WithholdingTax" type="XML"/>
				<core:Fragment fragmentName="com.spm.suppilerportal.view.subView.SupplierOnboarding.OtherInfo" type="XML"/>
				<core:Fragment fragmentName="com.spm.suppilerportal.view.subView.SupplierOnboarding.StatutoryDocuments" type="XML"/>
				<core:Fragment fragmentName="com.spm.suppilerportal.view.subView.SupplierOnboarding.ReviewLog" type="XML"/>
			</ScrollContainer>
			<HBox backgroundDesign="Transparent" class="bottombackground sapUiSmallPaddingBegin" justifyContent="SpaceBetween" width="100%">
				<Image src="/css/image/bottomlogo.PNG"/>
				<Text text="@FourOrigin" class="whiteText"/>
			</HBox>
		</content>
	</Page>
</mvc:View>